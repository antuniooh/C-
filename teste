#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
    int N =0;
    string num;

    cin >> N;
    int x=0;

    for (int i = 0; i < N; i++) {
        cin >> num;
        for (int j = 0; j < num.size()- 1; ++j) {
            if(num[j] == 'x') {
                string C = num.substr(0, j);
                string E = num.substr(j+1, j+1);

                cout << C <<"   " << E << endl;
                //unsigned long long int l1 = atof(L1.c_str());
            }
            cout << j << endl;

        }

    }





    return 0;
}
